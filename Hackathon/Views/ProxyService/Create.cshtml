            </div>
        </div>
        <div class="form-group">
            <label asp-for="ErrorPageTemplate" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="ErrorPageTemplate" class="form-control" />
                <span asp-validation-for="ErrorPageTemplate" class="text-danger"></span>
            </div>
        </div>
