            </div>
        </div>
        <div class="form-group">
            <label asp-for="KeepAliveTimeoutSeconds" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="KeepAliveTimeoutSeconds" class="form-control" />
                <span asp-validation-for="KeepAliveTimeoutSeconds" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="MaxHeaderSizeBytes" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="MaxHeaderSizeBytes" class="form-control" />
                <span asp-validation-for="MaxHeaderSizeBytes" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="MaxHeaderLines" class="col-md-2 control-label"></label>
