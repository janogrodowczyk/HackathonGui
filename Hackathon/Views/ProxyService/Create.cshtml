<link rel="stylesheet" href="~/css/bootstrap.css" />
<link rel="stylesheet" href="~/css/site.css" />
<link href="~/css/Personal.css" rel="stylesheet" />
@model Hackathon.Model.ProxyServiceViewModel

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Create</title>
</head>
<body>

<form asp-action="Create">
    <div class="form-horizontal">
        <h4>ProxyService</h4>
        <hr />
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group">
            <label asp-for="ServiceClusterName" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="ServiceClusterName" class="form-control" />
                <span asp-validation-for="ServiceClusterName" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="ServiceClusterId" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="ServiceClusterId" class="form-control" />
                <span asp-validation-for="ServiceClusterId" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="ListenPort" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="ListenPort" class="form-control" />
                <span asp-validation-for="ListenPort" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <div class="checkbox">
                    <input asp-for="AllowHttp" />
                    <label asp-for="AllowHttp"></label>
                </div>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="UnencryptedListenPort" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="UnencryptedListenPort" class="form-control" />
                <span asp-validation-for="UnencryptedListenPort" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="AuthenticationListenPort" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="AuthenticationListenPort" class="form-control" />
                <span asp-validation-for="AuthenticationListenPort" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <div class="checkbox">
                    <input asp-for="KerberosAuthentication" />
                    <label asp-for="KerberosAuthentication"></label>
                </div>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="SslBrowserCertificateThumbprint" class="col-md-2 control-label"></label>
