<link rel="stylesheet" href="~/css/bootstrap.css" />
<link rel="stylesheet" href="~/css/site.css" />
<link href="~/css/Personal.css" rel="stylesheet" />
@model Hackathon.Model.EngineServiceViewModel

@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
</head>
<body>

<form asp-action="Create">
    <div class="form-horizontal">
        <h4>EngineService</h4>
        <hr />
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group">
            <label asp-for="ServiceClusterName" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="ServiceClusterName" class="form-control" />
                <span asp-validation-for="ServiceClusterName" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="ServiceClusterId" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="ServiceClusterId" class="form-control" />
                <span asp-validation-for="ServiceClusterId" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <div class="checkbox">
                    <input type="checkbox" asp-for="OverlayDocuments" />
                    <label asp-for="OverlayDocuments"></label>
                </div>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="AutosaveInterval" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="AutosaveInterval" class="form-control" />
                <span asp-validation-for="AutosaveInterval" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="DocumentTimeout" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="DocumentTimeout" class="form-control" />
                <span asp-validation-for="DocumentTimeout" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="TableFilesDirectory" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="TableFilesDirectory" class="form-control" />
                <span asp-validation-for="TableFilesDirectory" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="DocumentDirectory" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="DocumentDirectory" class="form-control" />
                <span asp-validation-for="DocumentDirectory" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="GenericUndoBufferMaxSize" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="GenericUndoBufferMaxSize" class="form-control" />
                <span asp-validation-for="GenericUndoBufferMaxSize" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <div class="checkbox">
                    <input type="checkbox" asp-for="QvLogEnabled" />
                    <label asp-for="QvLogEnabled"></label>
                </div>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="GlobalLogMinuteInterval" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="GlobalLogMinuteInterval" class="form-control" />
                <span asp-validation-for="GlobalLogMinuteInterval" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="HostName" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="HostName" class="form-control" />
                <span asp-validation-for="HostName" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <label asp-for="ModifiedByUserName" class="col-md-2 control-label"></label>
            <div class="col-md-10">
                <input asp-for="ModifiedByUserName" class="form-control" />
                <span asp-validation-for="ModifiedByUserName" class="text-danger"></span>
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <div class="checkbox">
                    <input type="checkbox" asp-for="IdentifiedInternally" />
                    <label asp-for="IdentifiedInternally"></label>
                </div>
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" value="Create" class="btn btn-default" />
            </div>
        </div>
    </div>
</form>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
</body>
</html>
